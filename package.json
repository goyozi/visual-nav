{
  "name": "visual-nav",
  "displayName": "VisualNav",
  "description": "",
  "publisher": "Grzegorz Ziemonski",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "VisualNav configuration",
      "properties": {
        "visualNav.minimumGap": {
          "type": "number",
          "default": 2,
          "description": "Minimum number of characters between navigation points."
        },
        "visualNav.scrollStep": {
          "type": "number",
          "default": 5,
          "description": "Number of lines to scroll up/down in one step."
        }
      }
    },
    "commands": [
      {
        "command": "extension.toggleNav",
        "title": "Toggle Navigation"
      }
    ],
    "keybindings": [
      {
        "command": "extension.toggleNav",
        "key": "alt+a",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.onBackspace",
        "key": "Backspace",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
